{
  "name": "@preflower/element",
  "version": "0.2.0",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --target lib --name easy-element packages/index.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "release": "release-it"
  },
  "main": "dist/easy-element.common.js",
  "dependencies": {
    "core-js": "^3.6.5",
    "element-ui": "^2.15.6",
    "vue": "^2.6.14"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.8",
    "@commitlint/config-conventional": "^13.2.0",
    "@release-it/conventional-changelog": "^3.3.0",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/cli-plugin-babel": "~4.5.14",
    "@vue/cli-plugin-eslint": "~4.5.14",
    "@vue/cli-plugin-unit-jest": "~4.5.14",
    "@vue/cli-service": "~4.5.14",
    "@vue/eslint-config-standard": "^5.1.2",
    "@vue/test-utils": "^1.2.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "release-it": "^14.11.6",
    "vue-template-compiler": "^2.6.14"
  },
  "gitHooks": {
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": "https://github.com/preflower/easy-element",
  "author": "Ted <preflower.lin@gmail.com> (https://github.com/preflower)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/preflower/easy-element/issues"
  },
  "homepage": "https://github.com/preflower/easy-element#readme",
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/easy-element.common.js"
  ]
}
